<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.jwala.fundo.HomePage"
    android:background="#860404"
    android:clickable="false">

    <ImageView
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:id="@+id/calculator"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_margin="20dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/cal"
        android:clickable="false"
        android:longClickable="false"
        android:contextClickable="false" />

    <ImageView
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:id="@+id/camera"
        android:layout_alignTop="@+id/calculator"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:src="@drawable/camera" />

    <ImageView
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:id="@+id/quiz"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/calculator"
        android:layout_alignStart="@+id/calculator"
        android:layout_marginTop="40dp"
        android:src="@drawable/quiz" />

    <ImageView
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:id="@+id/game"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/camera"
        android:layout_alignStart="@+id/camera"
        android:src="@drawable/game" />

    <ImageView
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:id="@+id/media"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/quiz"
        android:layout_alignStart="@+id/quiz"
        android:layout_marginBottom="20dp"
        android:src="@drawable/media" />

    <ImageView
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:id="@+id/sensor"
        android:layout_alignBottom="@+id/media"
        android:layout_alignLeft="@+id/game"
        android:layout_alignStart="@+id/game"
        android:src="@drawable/sensor" />
</RelativeLayout>
